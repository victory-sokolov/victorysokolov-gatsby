{
    "compilerOptions": {
        "jsx": "react",
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "dom",
            "esnext"
        ],
        "baseUrl": "./",
        "noEmit": true,
        "strict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "noUnusedLocals": false,
        "allowJs": true,
        "explainFiles": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules",
        "public",
        ".cache"
    ],
    "include": [
        "./src/**/*",
        "./gatsby-node.ts",
        "./gatsby-config.ts",
        "./plugins/**/*",
        "functions/subscribe.js"
    ]
}
