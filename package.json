{
  "name": "viktor-sokolov-gatsby-blog",
  "private": true,
  "description": "Gatsby blog",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "cbuild": "gatsby clean && gatsby build",
    "develop": "tsc -p ./tsconfig-gatsby.json && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prepare": "husky install",
    "postinstall": "husky install",
    "lint:fix": "npm run lint --fix",
    "check-updates": "ncu -u",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/react-helmet": "^6.1.4",
    "dotenv": "^10.0.0",
    "gatsby": "^4.0.0",
    "gatsby-plugin-google-gtag": "^4.2.0",
    "gatsby-plugin-image": "^2.1.2",
    "gatsby-plugin-manifest": "^4.1.3",
    "gatsby-plugin-mdx": "^3.1.3",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-sharp": "^4.1.3",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-plugin-styled-components": "^5.1.0",
    "gatsby-plugin-typescript": "^4.0.0",
    "gatsby-remark-autolink-headers": "^5.0.0",
    "gatsby-remark-copy-linked-files": "^5.0.0",
    "gatsby-remark-images": "^6.1.3",
    "gatsby-source-filesystem": "^4.1.2",
    "gatsby-transformer-sharp": "^4.0.0",
    "mdx-utils": "^0.2.0",
    "normalize.css": "^8.0.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-plugin-styled-components": "^1.13.3",
    "eslint": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/victory-sokolov/victorysokolov"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run format",
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
